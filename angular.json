{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "canvas-gamification-ui": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/canvas-gamification-ui",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "allowedCommonJsDependencies": [
                            "dragula",
                            "lodash",
                            "graphlib",
                            "dagre",
                            "jquery",
                            "backbone"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            {
                                "glob": "**/*",
                                "input": "./node_modules/ace-builds/src/",
                                "output": "/"
                            },
                            {
                                "glob": "**/*",
                                "input": "node_modules/@taiga-ui/icons/src",
                                "output": "assets/taiga-ui/icons"
                            }
                        ],
                        "styles": [
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "node_modules/jointjs/css/layout.css",
                            "node_modules/dragula/dist/dragula.css",
                            "src/scss/material/global-material-theme.scss",
                            "src/scss/styles.scss",
                            "node_modules/@taiga-ui/core/styles/taiga-ui-global.less",
                            "node_modules/@taiga-ui/core/styles/taiga-ui-theme.less"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/scss",
                                "node_modules/bootstrap/scss"
                            ]
                        },
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.min.js",
                            "node_modules/lodash/lodash.js",
                            "node_modules/backbone/backbone.js",
                            "node_modules/jointjs/dist/joint.js",
                            "node_modules/ace-builds/src-min/ace.js",
                            "node_modules/ace-builds/src-min/mode-javascript.js",
                            "node_modules/ace-builds/src/theme-monokai.js",
                            "node_modules/ace-builds/src/mode-html.js",
                            "node_modules/@json-editor/json-editor/dist/jsoneditor.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "canvas-gamification-ui:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "canvas-gamification-ui:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "canvas-gamification-ui:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/scss/material/global-material-theme.scss",
                            "src/scss/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "src/scss",
                                "node_modules/bootstrap/scss"
                            ]
                        },
                        "scripts": [
                            "node_modules/jquery/dist/jquery.min.js",
                            "node_modules/bootstrap/dist/js/bootstrap.min.js",
                            "node_modules/lodash/lodash.js",
                            "node_modules/backbone/backbone.js",
                            "node_modules/jointjs/dist/joint.js",
                            "node_modules/ace-builds/src-min/ace.js",
                            "node_modules/ace-builds/src-min/mode-javascript.js",
                            "node_modules/ace-builds/src/theme-monokai.js",
                            "node_modules/ace-builds/src/mode-html.js",
                            "node_modules/@json-editor/json-editor/dist/jsoneditor.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "src/**/*.ts",
                            "src/**/*.html"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "canvas-gamification-ui:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "canvas-gamification-ui:serve:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "canvas-gamification-ui",
    "cli": {
        "analytics": "9de04249-6dbe-4103-b4f2-50ce895f19e6"
    }
}
